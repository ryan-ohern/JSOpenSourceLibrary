// ARRAYS - CONCAT

var favoriteBands = ["Ben Rector", "needtobreathe", "Queen", "Beatles"];

console.log(favoriteBands);

// combines array into string
var concatBands = favoriteBands.join();

console.log(concatBands);

// combines array into string without separator
var concatBands = favoriteBands.join("");

console.log(concatBands);