//OBJECT CONSTRUCTORS

function person (first, last, age, eye){
	this.firstName = first;
	this.lastName = last;
	this.age = age;
	this.eyeColor = eye;
}

var myFather = new person("Tim", "O'Hern", 60, "blue");
var myMother = new person("Marla", "O'Hern", 58, "brown");
console.log(myFather);
console.log(myMother);